{
  "name": "lq-element-ui-2",
  "version": "0.0.16",
  "scripts": {
    "serve": "vue-cli-service serve src/dev/index.js",
    "build-bundle": "vue-cli-service build --target lib --name lq.element.ui src/main.js",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "test": "jest"
  },
  "license": "MIT",
  "main": "dist/lq.element.ui.umd.js",
  "dependencies": {},
  "files": [
    "dist/*",
    "src/*"
  ],
  "devDependencies": {
    "@google/maps": "^1.0.2",
    "@vue/cli-plugin-babel": "^3.10.0",
    "@vue/cli-plugin-eslint": "^3.10.0",
    "@vue/cli-service": "^3.10.0",
    "@vue/test-utils": "^1.0.0-beta.29",
    "axios": "^0.19.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.9.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-minify": "^0.5.1",
    "core-js": "^2.6.5",
    "date-fns": "^2.0.1",
    "element-ui": "^2.13.0",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "jest": "^24.9.0",
    "jest-serializer-vue": "^2.0.2",
    "jsdom": "^15.2.1",
    "jsdom-global": "^3.0.2",
    "lq-form": "^0.1.116",
    "lq-v-file": "^0.1.42",
    "lq-vuetify": "^0.0.61",
    "mdi": "^2.2.43",
    "moment": "^2.29.4",
    "node-sass": "^4.9.0",
    "sass-loader": "^7.1.0",
    "sinon": "^7.5.0",
    "validate.js": "^0.13.1",
    "vue": "^2.6.10",
    "vue-croppie": "^2.0.1",
    "vue-google-autocomplete": "^1.1.0",
    "vue-jest": "^3.0.5",
    "vue-phone-number-input": "^1.1.0",
    "vue-router": "^3.0.3",
    "vue-template-compiler": "^2.6.10",
    "vuejs-object-helper": "^0.1.31",
    "vuex": "^3.0.1",
    "webpack-node-externals": "^1.7.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "vue"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "<rootDir>/src/components/**/*.{js,vue}",
      "!**/node_modules/**"
    ],
    "transform": {
      ".*\\.(vue)$": "vue-jest",
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest"
    },
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1",
      "vuejs-object-helper": "<rootDir>/node_modules/vuejs-object-helper/dist/lq.helper.umd.js"
    },
    "snapshotSerializers": [
      "jest-serializer-vue"
    ],
    "setupFiles": [
      "<rootDir>/jest/global.js"
    ]
  }
}
